{
  "extends": [
    "config:base",
    ":automergePatch"
  ],
	 "packageRules": [
    {
      "updateTypes": ["major"],
      "addLabels": ["major"]
    },
    {
      "updateTypes": ["minor"],
      "addLabels": ["minor"]
    },
     {
      "updateTypes": ["patch"],
      "addLabels": ["patch"]
    },
    {
      "updateTypes": ["pin"],
      "addLabels": ["pin"]
    }
   ],
  "baseBranches": ["dev"],
  "labels": ["dependencies"],
  "branchPrefix": "deps/",
  "bumpVersion": "minor",
  "rebaseWhen": "behind-base-branch",		
  "commitBodyTable": "true",
  "commitMessagePrefix": "feat(Dependencies):"
}
